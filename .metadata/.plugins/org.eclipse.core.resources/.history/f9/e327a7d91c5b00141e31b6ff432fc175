  <select id="selectProductCommissionByProductId" resultMap="BaseResultMap" parameterType="java.lang.String" >
	select ju.REAL_NAME PUBLISHER_NAME,ju.LOGIN_NAME LOGIN_NAME, jpc.* ,jp.SHORT_NAME PRODUCT_NAME
			from jmt_product_commission jpc,jmt_user ju,jmt_product jp
		where jpc.PUBLISHER_ID=ju.ID
		and jpc.PRODUCT_ID=jp.ID
		and jpc.PRODUCT_ID=#{productId,jdbcType=VARCHAR};
  </select>
  <select id="selectAllProductCommission" resultMap="BaseResultMap" >
